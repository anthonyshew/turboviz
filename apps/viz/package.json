{
  "name": "@ashew/turboviz",
  "description": "A visualization studio to help with your Turborepo.",
  "author": {
    "name": "Anthony Shew",
    "url": "https://twitter.com/anthonysheww"
  },
  "version": "0.0.7-canary.0",
  "main": "dist/bin.js",
  "bin": "dist/bin.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "NODE_ENV=development concurrently \"tsx watch ./bin.ts\" \"next dev -p 3000\"",
    "build:ui": "next build && next export",
    "package:ui": "mkdir -p dist && rm -rf dist/out && cp -R ./out dist/out/",
    "package:server": "tsup",
    "build": "pnpm build:ui && pnpm package:ui && pnpm package:server",
    "start": "NODE_ENV=production node dist/bin.js",
    "lint": "next lint",
    "test": "echo \"Tests go here!\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "open": "^8.4.2",
    "portfinder": "^1.0.32",
    "serve-handler": "^6.1.5"
  },
  "devDependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^1.2.1",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "18.14.6",
    "@types/react-dom": "18.0.11",
    "@types/react": "18.0.28",
    "@types/serve-handler": "^6.1.1",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.2",
    "concurrently": "^7.6.0",
    "eslint-config-custom": "workspace:*",
    "eslint": "8.35.0",
    "dagre": "^0.8.5",
    "express": "^4.18.2",
    "postcss": "^8.4.21",
    "next": "13.2.5-canary.33",
    "react-dom": "^18.2.0",
    "react": "^18.2.0",
    "reactflow": "^11.5.6",
    "tailwindcss": "^3.2.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2",
    "ui": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/anthonyshew/turboviz/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anthonyshew/turboviz/issues"
  }
}
